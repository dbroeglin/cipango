<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">
   
	<servlet>
		<servlet-name>oam</servlet-name>
		<servlet-class>org.cipango.example.OamServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ClickToDialHttpServlet</servlet-name>
		<servlet-class>org.cipango.example.ClickToDialHttpServlet</servlet-class>
	</servlet>

   <servlet-mapping>
		<servlet-name>oam</servlet-name>
		<url-pattern>/oam</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ClickToDialHttpServlet</servlet-name>
		<url-pattern>/call</url-pattern>
	</servlet-mapping>
</web-app>
